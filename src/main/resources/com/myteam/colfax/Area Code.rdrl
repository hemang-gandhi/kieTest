package com.myteam.colfax;

import java.lang.Number;

rule "Area Code"
	dialect "mvel"
	salience 10
	when
		colfax : ColfaxInfoArmorAttributes( homePhone : homePhone != null , homePhone != "" )
	then
		ColfaxInfoArmorAttributes colfaxInfoArmor = new ColfaxInfoArmorAttributes();
		insert( colfaxInfoArmor );
		colfaxInfoArmor.getDobSubString( homePhone, 0, 3 );
		colfax.setAreaCode( colfaxInfoArmor.getResult() );
end

package com.myteam.colfax;

import java.lang.Number;

rule "Family ID"
	dialect "mvel"
	when
		colfax : ColfaxInfoArmorAttributes( employeeAssignedID : employeeAssignedID != null )
	then
		CommonUtil cu = new CommonUtil();
		insert( cu );
		cu.getFamilyId( employeeAssignedID, colfax, "familyId" );
end
